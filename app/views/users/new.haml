%section
  .wrapper
    %h1 Register
    %p
      Already have any account?
      = link_to 'Login', new_session_url
    = form_for :user, url: users_url do |f|
      .field
        = f.label :email
        = f.text_field :email
      .field
        = f.label :password
        = f.password_field :password
      .actions
        = button_tag 'Register'
